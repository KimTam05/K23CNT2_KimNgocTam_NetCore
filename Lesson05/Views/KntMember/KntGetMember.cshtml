@{
    ViewData["Title"] = "Get Member";
    var members = ViewBag.Members;
}
<div class="text-center">
    <h1 class="display-4">Get Member</h1>
    <table class="table table-bordered table-striped table-hover">
        <thead>
            <tr>
                <th>Member ID</th>
                <th>User Name</th>
                <th>Full Name</th>
                <th>Password</th>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var member in members)
            {
                <tr>
                    <td>@member.KntMemberId</td>
                    <td>@member.KntUserName</td>
                    <td>@member.KntFullName</td>
                    <td>@member.KntPassword</td>
                    <td>@member.KntEmail</td>
                </tr>
            }
        </tbody>
    </table>
</div>
